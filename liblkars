#!/bin/bash
# Lkcars


TERM=ansi
src_dir="$HOME/.local/src"

list_function()
{
  ( grep '.*()' '/lib/liblkars' | sed -z 's|()| |g;s|\n|,|g;s|,$|\n|' )
}

set_samba()
{
  sudo apt install samba -y
  echo "change workgroup name"
  #add sed
  sudo nano /etc/samba/smb.conf
  echo "enter a password"
  sudo smbpasswd -a alpha
  sudo service smbd restart
}

rem_samba()
{
  sudo apt-get purge samba
  sudo rm -rf /etc/samba/ /etc/default/samba
}

set_iic()
{
  i=$( ls /dev/i2c* ) ;
  [[ $i != /dev/i2c-1 ]] && sudo raspi-config nonint do_i2c 0 ;
}

installlkars()
{
  ( git --version || sudo apt install git )
  [ ! -d "$src_dir/lkars/" ] && ( git clone https://github.com/Tearran/lkars.git "$src_dir/lkars" || echo "LKARS src Exist" )
  [ ! -d "$src_dir/lkars/" ] && ( cd "$src_dir/lkars" || exit 1 )
  ( sudo install -b -v "$src_dir/lkars/liblkars" /usr/lib/ && sudo install -b -v "$src_dir/lkars/lkars-config" /usr/bin/ )
}

uninstall()
{
  sudo rm -f  /usr/lib/liblkars*
  sudo rm -f  /usr/bin/lkars-config*
  echo "uninstall complete!"
}

dev()
{
  echo
  echo "TODO"
  echo
}

demo_ansi()
{
  clear
  for x in {0..7} ; 
  do
    #echo -e "\e[H"
    echo "ESCAPE=\e[$x " 
      for (( y=0; y<=9; y++ )) ; 
      do
        for (( z=0; z<=9; z++ )) ; 
        do
          printf "\e[${x};3${y};4${z}m"
          printf "\\\\\\\e[${x};3${y};4${z}m"
          printf "\e[0m "
        done
      echo
      done
    echo
  done
echo
}

