vers()
{
  echo " "
  echo "LKARS lib."
  echo
  echo "Software version: pre-Alpha."
  echo
}

help()
{
  echo -e ""
  echo -e "Useage: liblkars [-h|-v|-i|-u]"
  echo -e "Options:"
  echo -e "\t-d\tDemo, Retro art"
  echo -e "\t-i\tInstall lkars-config"
  echo -e "\t-u\tUninstall lkars-config"
  echo -e "\t-h\tPrint this help."
  echo -e "\t-v\tPrint version info."
}

if [[ $# -eq 0 ]] ; then
  #installlkars
  help
  exit 1
fi


while getopts ":hlviuda" opt; do
    case $opt in
      h) help break ;;
      l) list_function break ;;
      v) vers break ;;
      i) installlkars break ;;
      u) uninstall break ;;
      d) demo_01  break ;;
      ?) help break ;;
    esac
  exit
done
